<html lang="pt-BR">
<head>
  <meta charset="utf-8" />
  <title>Mapa Interativo - Regi√µes de MG</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <link rel="stylesheet" href="${BASE_FOLDER}/bootstrap/css/bootstrap.min.css" />
  <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"></script>
  <link rel="stylesheet" href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css" />
  <snk:load/>

  <style>
    html, body { height: 100%; margin: 0; font-family: Arial, sans-serif; }
    #container { display: flex; height: 100%; }
    #sidebar {
      width: 350px;
      padding: 15px;
      overflow-y: auto;
      background: #f9f9f9;
      border-right: 1px solid #ccc;
    }
    #map { flex: 1; }
  </style>

</head>
<body>
<div id="container">
  <div id="map"></div>
  <div id="sidebar">
    <h4 class="mb-3">Vendas por Regi√£o</h4>
    <div class="table-responsive">
      <table class="table table-striped table-sm">
        <thead class="table-dark">
          <tr>
            <th>Regi√£o</th>
            <th class="text-end">Total Vendas</th>
          </tr>
        </thead>
        <tbody>
          <snk:query var="lista"> <!-- Aqui √© onde a query do banco vai-->

          </snk:query>

          <c:forEach var="row" items="${lista.rows}">
            <tr>
              <td><c:out value="${row.REGIAO}"/></td>
              <td class="text-end">
                R$ <fmt:formatNumber value="${row.TOTAL_VENDAS}" type="number" minFractionDigits="2" maxFractionDigits="2"/>
              </td>
            </tr>
          </c:forEach>
        </tbody>
      </table>
    </div>
    <div class="mt-3 text-center">
      <button type="button" class="btn btn-success" onclick="openLevel('0I6', {});"> Cidades s/ regi√£o</button>
    </div>
    
    <snk:query var="alertaCidades">
      SELECT COUNT(*) AS QTD_CIDADES
      FROM TGFCAB CAB
      INNER JOIN TSICID CID ON CAB.CODCID = CID.CODCID
      INNER JOIN TSIUFS UFS ON CID.UF = UFS.CODUF
      WHERE CID.AD_REGIAODEMINAS IS NULL
        AND UFS.UF = 'MG'
    </snk:query>

    <c:forEach var="row" items="${alertaCidades.rows}">
      <c:if test="${row.QTD_CIDADES > 0}">
        <div class="alert alert-warning mt-2 text-center" role="alert">
          Existem cidades com vendas sem regi√£o cadastrada!
        </div>
      </c:if>
    </c:forEach>
  </div>
</div>

